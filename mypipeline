
  
pipeline {
    agent any 
    stages {
        stage('pull') { 
            steps {
                git 'https://github.com/samratkasarla/studentapp-ui.git'
            }
        }
        stage('Test') { 
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Deploy') { 
            steps {
                echo "hey"
            }
        }
    }
}