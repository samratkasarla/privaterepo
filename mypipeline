pipeline {
    agent any 
    stages {
        stage('pull') { 
            steps {
                sh 'git pull https://github.com/samratkasarla/branchdemo.git master' 
            }
        }
        stage('Test') { 
            steps {
                echo "hi"
            }
        }
        stage('Deploy') { 
            steps {
                echo "hey"
            }
        }
    }
}
